public class arraycopy {
    public static void main(String[] args) {
        String[] arr1 = {"liuyapeng","lijingjing"};
        String[] arr2 = new String[10];

        System.arraycopy(arr1,0,arr2,0,2);
        for (int i = 0; i < arr2.length; i++) {
            System.out.println(arr2[i]);
        }
    }
}
